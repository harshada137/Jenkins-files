pipeline {
    agent any

    stages {
        stage('Pull') {
            steps {
                git branch: 'main', url: 'https://github.com/iamtruptimane/CICD-with-AWS-code-services.git'            }
        }
        
         stage('Build') {
            steps {
                echo 'Building the repo'
            }
        }
        
         stage('Test') {
            steps {
                echo 'Testing the Project'
            }
        }
        
         stage('Deploy') {
            steps {
                echo 'Deploying The Project'
            }
        }
    }
}
